.modal{
  position: fixed;
  top:0;
  right: 0;
  bottom:0;
  left: 0;
  z-index: 99;
  text-align: center;
  background:rgba(0,0,0,0.6);
}
.panel{
  position: absolute;
  bottom:0rpx;
  left:0;
  width:100%;
  min-height: 300rpx;
  box-sizing: border-box;
  background-color:#f8f8f8;
}
.delete{
  position:absolute;
  width: 50rpx;
  height: 50rpx;
  right: 10rpx;
  top:10rpx;
  padding:15rpx;
}